#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")
java -Djna.nosys=true -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/MakerLink_MAC.jar:$APPDIR/lib/core.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all-natives-linux-aarch64.jar:$APPDIR/lib/gluegen-rt-natives-linux-aarch64.jar:$APPDIR/lib/websocket-common-9.3.6.v20151106.jar:$APPDIR/lib/jetty-util-9.3.6.v20151106.jar:$APPDIR/lib/jetty-http-9.3.6.v20151106.jar:$APPDIR/lib/websocket-server-9.3.6.v20151106.jar:$APPDIR/lib/websocket-api-9.3.6.v20151106.jar:$APPDIR/lib/websocket-servlet-9.3.6.v20151106.jar:$APPDIR/lib/javax.servlet-api-3.1.0.jar:$APPDIR/lib/websocket-client-9.3.6.v20151106.jar:$APPDIR/lib/jetty-server-9.3.6.v20151106.jar:$APPDIR/lib/webSockets.jar:$APPDIR/lib/jetty-io-9.3.6.v20151106.jar:$APPDIR/lib/jetty-security-9.3.6.v20151106.jar:$APPDIR/lib/jetty-servlet-9.3.6.v20151106.jar:$APPDIR/lib/slf4j-simple-1.7.13.jar:$APPDIR/lib/slf4j-api-1.7.13.jar:$APPDIR/lib/jssc.jar:$APPDIR/lib/serial.jar:$APPDIR/lib/controlP5.jar" MakerLink_MAC "$@"
